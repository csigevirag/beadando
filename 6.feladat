def kisebb(n):
    n=int(input("Adjon meg egy 1000-nél kisebb számot:"))
    s=0
    t=[0,4,7]
    li=[1,2,3,5,6,8,9]
    if int(n)<=1000:
        for num in range(0,n+1):
            nu=map(int,str(num))
            for m in nu:
                if m  in t:
                    s+=1
        return s

